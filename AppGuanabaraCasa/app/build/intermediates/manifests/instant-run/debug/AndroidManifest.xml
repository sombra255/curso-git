<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.fabricio.appguanabaracasa"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="24" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="br.com.fabricio.appguanabaracasa.activities.CompraActivity"
            android:label="@string/comprarPassagens" />
        <activity
            android:name="br.com.fabricio.appguanabaracasa.activities.ListaAlunoActivity"
            android:label="@string/agenda" />
        <activity android:name="br.com.fabricio.appguanabaracasa.MainActivity" />
        <activity
            android:name="br.com.fabricio.appguanabaracasa.activities.FormularioActivity"
            android:label="@string/formulario" />
        <activity
            android:name="br.com.fabricio.appguanabaracasa.activities.SplashScreenActivity"
            android:label="@string/title_activity_splash_screen"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name="br.com.fabricio.appguanabaracasa.FrameActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>