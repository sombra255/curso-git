<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.fabricio.appguanabaracasa">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.CompraActivity"
            android:label="@string/comprarPassagens" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name=".activities.ListaAlunoActivity"
            android:label="@string/agenda">
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".activities.FormularioActivity"
            android:label="@string/formulario"></activity>
    </application>

</manifest>